{
  "name": "@afforai/citation-js-plugin-isbn",
  "version": "0.4.1",
  "description": "Citation.js plugin to fetch ISBN metadata",
  "main": "lib/index.js",
  "scripts": {
    "test": "mocha -r @babel/register -R spec test/suite.js",
    "babel": "babel src -d lib --copy-files",
    "lint": "standard \"src/**/*.js\" \"test/**/*.js\"",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s -r 0 && git add CHANGELOG.md",
    "coverage": "NODE_ENV=coverage nyc npm test",
    "report": "nyc report --reporter=lcov > coverage.lcov",
    "prepublishOnly": "npm run babel",
    "version": "npm run changelog",
    "preversion": "npm run lint && npm run test"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/afforai/plugin-isbn.git"
  },
  "keywords": [
    "citation.js",
    "ISBN",
    "Google Books",
    "Open Library"
  ],
  "author": "Lars Willighagen",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/afforai/plugin-isbn/issues"
  },
  "homepage": "https://github.com/afforai/plugin-isbn#readme",
  "engines": {
    "node": ">=14"
  },
  "files": [
    "/lib"
  ],
  "nyc": {
    "include": [
      "src/**/*.js"
    ],
    "sourceMap": false,
    "instrument": false
  },
  "standard": {
    "parser": "@babel/eslint-parser"
  },
  "dependencies": {
    "@citation-js/date": "^0.5.0",
    "@citation-js/name": "^0.4.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.2.2",
    "@babel/eslint-parser": "^7.14.3",
    "@babel/preset-env": "^7.2.3",
    "@babel/register": "^7.0.0",
    "@afforai/citation-js-core": "^0.7.17",
    "babel-plugin-istanbul": "^6.0.0",
    "conventional-changelog-cli": "^3.0.0",
    "mocha": "^10.2.0",
    "nyc": "^15.1.0",
    "standard": "^17.1.0"
  },
  "peerDependencies": {
    "@afforai/citation-js-core": ">=0.7.0"
  }
}
